<?xml version="1.0" ?>
<resource>
  <object class="wxDialog" name="ConfigRemote">
    <object class="wxBoxSizer">
      <orient>wxVERTICAL</orient>
      <object class="sizeritem">
        <object class="wxStaticBoxSizer">
          <object class="sizeritem">
            <object class="wxFlexGridSizer">
              <object class="sizeritem">
                <object class="wxStaticText">
                  <label>User Name :</label>
                </object>
                <option>1</option>
                <border>0</border>
              </object>
              <object class="sizeritem">
                <object class="wxComboBox" name="user">
                  <selection>0</selection>
                  <value>ishikura</value>
                </object>
                <option>1</option>
                <flag>wxALL|wxEXPAND</flag>
              </object>
              <object class="sizeritem">
                <object class="wxStaticText">
                  <label>Remote Host : </label>
                </object>
                <border>0</border>
              </object>
              <object class="sizeritem">
                <object class="wxComboBox" name="host">
                  <value>133.66.117.139</value>
                </object>
                <option>1</option>
                <flag>wxALL|wxEXPAND</flag>
              </object>
              <object class="sizeritem">
                <object class="wxStaticText">
                  <label>Password :</label>
                </object>
                <border>0</border>
              </object>
              <object class="sizeritem">
                <object class="wxTextCtrl" name="passwd">
                  <style>wxTE_PASSWORD</style>
                </object>
                <flag>wxEXPAND</flag>
              </object>
              <object class="sizeritem">
                <object class="wxStaticText">
                  <label>Port :</label>
                </object>
                <border>0</border>
              </object>
              <object class="sizeritem">
                <object class="wxComboBox" name="port">
                  <value>22</value>
                  <enabled>0</enabled>
                </object>
              </object>
              <cols>2</cols>
              <rows>5</rows>
              <vgap>5</vgap>
              <hgap>20</hgap>
            </object>
            <option>1</option>
            <flag>wxALL|wxEXPAND</flag>
            <border>5</border>
          </object>
          <label>SSH remote access setting</label>
          <orient>wxVERTICAL</orient>
        </object>
        <option>1</option>
        <flag>wxALL|wxEXPAND</flag>
        <border>3</border>
      </object>
      <object class="sizeritem">
        <object class="wxCheckBox" name="save">
          <label>save remote setting</label>
          <checked>1</checked>
        </object>
        <flag>wxLEFT</flag>
        <border>5</border>
      </object>
      <object class="sizeritem">
        <object class="wxBoxSizer">
          <orient>wxHORIZONTAL</orient>
          <object class="sizeritem">
            <object class="wxButton" name="ok">
              <label>Ok</label>
            </object>
            <flag>wxALL</flag>
            <border>3</border>
          </object>
          <object class="sizeritem">
            <object class="wxButton" name="cancel">
              <label>Cancel</label>
            </object>
            <flag>wxALL</flag>
            <border>3</border>
          </object>
        </object>
        <flag>wxALL|wxALIGN_RIGHT</flag>
        <border>3</border>
      </object>
    </object>
  </object>
</resource>